{
  "name": "react-compose-state",
  "description": "A helper function to attach state to stateless function components",
  "version": "1.0.0",
  "author": "Daishi Kato <daishi@axlight.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/react-compose-state.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "compile": "babel src -d dist",
    "test": "npm run eslint && npm run jest",
    "eslint": "eslint src example test",
    "jest": "jest",
    "example": "webpack-dev-server --port ${PORT:-8080} --content-base example"
  },
  "keywords": [
    "react",
    "container",
    "state",
    "compose",
    "stateless",
    "thisless",
    "pure"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": "^15.3.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
