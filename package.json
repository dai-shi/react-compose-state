{
  "name": "react-compose-state",
  "description": "A helper function to attach state to stateless function components",
  "version": "1.2.0",
  "author": "Daishi Kato <daishi@axlight.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/react-compose-state.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "compile": "babel src -d dist",
    "unix-pretest": "killall flow",
    "test": "npm run flow && npm run eslint && npm run jest",
    "eslint": "eslint src example test",
    "flow": "flow",
    "jest": "jest",
    "example": "webpack-dev-server --port ${PORT:-8080} --content-base example"
  },
  "keywords": [
    "react",
    "container",
    "state",
    "compose",
    "stateless",
    "thisless",
    "pure"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.6.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.81.0",
    "jest": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "peerDependencies": {
    "react": ">=15.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
